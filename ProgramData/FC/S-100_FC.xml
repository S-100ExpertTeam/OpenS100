			<S100FC:subAttributeBinding sequential="false">
				<S100FC:multiplicity>
					<S100Base:lower>0</S100Base:lower>
					<S100Base:upper xsi:nil="false" infinite="false">1</S100Base:upper>
				</S100FC:multiplicity>
				<S100FC:attribute ref="displayName" />
			</S100FC:subAttributeBinding>
			<S100FC:subAttributeBinding sequential="false">
